{
  "name": "mpldoctoc",
  "version": "0.1",
  "description": "Navigation TOC tree for matplotlib api documentation",
  "icons": { "16": "toc.png" },
  "content_scripts": [
    {
        "matches":["http://matplotlib.sourceforge.net/*"],
        "js": ["jquery-1.7.1.min.js", "toc.js"]
    }
  ],
  "permissions":["tabs","http://matplotlib.sourceforge.net/*"],
  "background_page":"bg.html",
  "page_action": {
    "default_icon": "toc.png",
    "default_title": "TOC",
    "default_popup": "popup.html"
  }
}
